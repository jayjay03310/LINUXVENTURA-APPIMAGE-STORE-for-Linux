{
  "name": "wii-iso-downloader",
  "version": "1.0.0",
  "description": "Baixador autom√°tico de ISOs de Nintendo Wii",
  "main": "main.js",
  "author": "Artxiesta",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --linux AppImage"
  },
  "build": {
    "appId": "com.wii.downloader",
    "productName": "Wii Downloader Channel",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Game",
      "maintainer": "Artxiesta",
      "description": "Baixe jogos de Wii direto do Archive.org"
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!dist/*",
      "!node_modules/.cache/*"
    ]
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0"
  }
}